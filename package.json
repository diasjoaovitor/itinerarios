    "dotenv-expand": "^11.0.6",
    "mysql2": "^3.11.3",
    "next": "^14.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "styled-components": "^6.1.13",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.38.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bluebird": "^3.5.42",
