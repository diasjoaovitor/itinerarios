    "eslint": "^8",
    "eslint-config-next": "^14.2.2",
    "eslint-config-prettier": "^9.1.0",
    "git-commit-msg-linter": "^5.0.7",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
